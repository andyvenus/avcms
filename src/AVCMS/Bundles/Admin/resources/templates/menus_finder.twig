{% extends '@admin/finder.twig' %}

{% block finder_header_titles %}

{% endblock %}

{% block item_title %}
    <div class="finder-item-title" data-field="label">
        {{ item.label }}
    </div>
{% endblock %}

{% block item_options %}
    <div class="btn-group">
        {{ buttons.manage(bundle.route.menus_admin_manage_items, item.id) }}
        {{ buttons.edit(bundle.route.menus_admin_edit, item.id) }}
        {% if item.custom == 1 %}
            {{ buttons.delete(bundle.route.menus_admin_delete) }}
        {% endif %}
    </div>
{% endblock %}

{% block item_extended_inner %}

{% endblock %}