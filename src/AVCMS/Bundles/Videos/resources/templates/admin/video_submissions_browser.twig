{% extends '@admin/browser.twig' %}

{% block finder_title %}
    {% trans %}Video Submissions{% endtrans %}
{% endblock %}

{% block finder_filters -%}{%- endblock %}

{% block expand_finder_button %}
    {{ buttons.expand_finder('video_submissions_admin_home') }}
{% endblock %}

{% block finder_extra_class %}
    finder-wide
{% endblock %}

{% block finder %}
    <div data-url="{{ path('video_submissions_admin_finder') }}">
        {{ render(path('video_submissions_admin_finder')) }}
    </div>
{% endblock %}

{% block mobile_back_link %}
    {{ path('video_submissions_admin_home') }}
{% endblock %}

{% block checked_options_buttons %}
    {{ buttons.bulk_delete('video_submissions_admin_delete') }}
{% endblock %}
