{
    "name": "avcms/avcms",
    "description": "AVCMS is a CMS framework for AV Scripts products",
    "require": {
        "symfony/http-foundation": "2.3.*",
        "symfony/routing": "2.3.*",
        "symfony/http-kernel": "2.3.*",
        "symfony/event-dispatcher": "2.3.*",
        "symfony/dependency-injection": "2.3.*",
        "symfony/form": "2.3.*",
        "symfony/translation": "2.3.*",
        "usmanhalalit/pixie": "1.*@dev",
        "twig/twig": "1.*",
        "symfony/web-profiler-bundle": "2.3.x-dev"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "phpmd/phpmd" : "1.4.*"
    },
    "autoload": {
        "psr-0": { "AVCMS": "src/", "Games": "src/" }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist"
    }
}
