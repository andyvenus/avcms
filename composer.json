{
    "name": "avcms/avcms",
    "description": "AVCMS is a CMS framework for AV Scripts products",
    "require": {
        "symfony/http-foundation": "2.5.*",
        "symfony/routing": "2.5.*",
        "symfony/http-kernel": "2.5.*",
        "symfony/event-dispatcher": "2.5.*",
        "symfony/dependency-injection": "2.5.*",
        "symfony/translation": "2.5.*",
        "symfony/twig-bridge": "2.5.*",
        "symfony/config": "2.5.*",
        "symfony/yaml": "2.5.*",
        "symfony/stopwatch": "2.5.*",
        "usmanhalalit/pixie": "1.*@dev",
        "twig/twig": "1.*",
        "swiftmailer/swiftmailer": "5.*",
        "kriswallsmith/assetic": "1.2.*-dev",
        "aptoma/twig-markdown": "0.2.*",
        "michelf/php-markdown": "1.3",
        "patchwork/jsqueeze": "1.0.*",
        "leafo/scssphp": "dev-master",
        "guzzlehttp/guzzle": "3.*",
        "filp/whoops": "1",
        "monolog/monolog": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/dbunit": "1.3.1",
        "phpmd/phpmd" : "1.4.*",
        "jms/cg": "1.1.*@dev",
        "mikey179/vfsStream": "1.3.*@dev"
    },
    "autoload": {
        "psr-4": { "AVCMS\\": "src/AVCMS/", "AVBlog\\": "src/AVBlog/" }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist"
    }
}
