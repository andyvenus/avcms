{
    "name": "avcms/avcms",
    "description": "AVCMS is a CMS framework for AV Scripts products",
    "license": "MIT",
    "type": "project",
    "require": {
        "andyvenus/form": "dev-master",
        "symfony/http-foundation": "2.6.*",
        "symfony/routing": "2.6.*",
        "symfony/http-kernel": "2.6.*",
        "symfony/event-dispatcher": "2.6.*",
        "symfony/dependency-injection": "2.6.*",
        "symfony/translation": "2.6.*",
        "symfony/twig-bridge": "2.6.*",
        "symfony/config": "2.6.*",
        "symfony/yaml": "2.6.*",
        "symfony/stopwatch": "2.6.*",
        "symfony/security": "2.6.*",
        "usmanhalalit/pixie": "dev-master#672e99f",
        "twig/twig": "1.*",
        "swiftmailer/swiftmailer": "5.*",
        "kriswallsmith/assetic": "1.2.*-dev",
        "patchwork/jsqueeze": "1.0.*",
        "filp/whoops": "1.1.*",
        "monolog/monolog": "1.*",
        "ircmaxell/password-compat": "1.0.*",
        "cocur/slugify": "@stable",
        "twig/extensions": "~1.1.0",
        "intervention/image": "~2.0",
        "php-curl-class/php-curl-class": "dev-master",
        "facebook/php-sdk-v4" : "4.0.*",
        "jms/cg": "1.1.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "phpunit/dbunit": "1.3.*",
        "phpmd/phpmd" : "1.4.*",
        "mikey179/vfsStream": "1.3.*@dev",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": { "AVCMS\\": "src/AVCMS/", "AV\\": "src/AV/" }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist"
    }
}
